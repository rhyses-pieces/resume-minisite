{
  "name": "vite-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "watch:assets": "vite build --watch.buildDelay 2000",
    "watch:twee": "node_modules/tweego-bin/bin/tweego -w -f sugarcube-2 -o index.html src/twee --head=src/head.html",
    "start": "npm-run-all -p watch:* serve"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/twine-sugarcube": "^2.34.2",
    "autoprefixer": "^10.3.4",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.41.1",
    "stylus": "^0.55.0",
    "tweego-bin": "^0.2.1",
    "vite": "^2.5.10"
  }
}
